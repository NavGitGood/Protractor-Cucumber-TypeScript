{
    "rulesDirectory": [
        "node_modules/tslint-lines-between-class-members",
        "node_modules/tslint-no-unused-expression-chai"
    ],
    "rules": {
        "arrow-parens": [
            true, "ban-single-arg-parens"
        ],
        "arrow-return-shorthand": true,
        "await-promise": true,
        "callable-types": true,
        "class-name": true,
        "comment-format": [
            true, "check-space"
        ],
        "curly": true,
        "eofline": true
    }
}
